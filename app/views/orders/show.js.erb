// Append new data
$("<%=j render(:partial => 'orders', :object => @orders) %>")
  .appendTo($(".infinite-table table"));

// Update pagination link
<% if @orders.last_page? %>
  $('.pagination').html("");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@orders, 'Next Page', :remote => true) %>");
<% end %>